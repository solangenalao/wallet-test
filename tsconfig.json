{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "es2017",
    "strict": true,
    "jsx": "react-native",
    "lib": ["es6"],
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "isolatedModules": true,
    "noEmit": true,
    "allowJs": true,
  },
  "exclude":["node_modules"]
}
